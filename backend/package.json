{
  "name": "skylark-backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init --skip-seed",
    "seed": "ts-node src/seed.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.4",
    "@prisma/client": "^6.16.2",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "prisma": "^6.16.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
